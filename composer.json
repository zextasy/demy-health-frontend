{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "althinect/filament-spatie-roles-permissions": "^2.0",
        "archtechx/enums": "^0.3.0",
        "darryldecode/cart": "^4.2",
        "doctrine/dbal": "^3.3",
        "filament/filament": "^3.2",
        "filament/spatie-laravel-media-library-plugin": "^3.2",
        "filament/spatie-laravel-settings-plugin": "^3.2",
        "filament/spatie-laravel-tags-plugin": "^3.2",
        "flowframe/laravel-trend": "^0.1.1",
        "guzzlehttp/guzzle": "^7.4",
        "jantinnerezo/livewire-alert": "^3.0",
        "konnco/filament-import": "^1.4",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^4.0",
        "leandrocfe/filament-apex-charts": "^3.0",
        "livewire/livewire": "^3.0",
        "maatwebsite/excel": "^3.1",
        "predis/predis": "^1.1",
        "realrashid/sweet-alert": "^7.0",
        "saade/filament-fullcalendar": "^3.0",
        "spatie/laravel-activitylog": "^4",
        "spatie/laravel-medialibrary": "^10.0",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-ray": "^1.29",
        "spatie/laravel-searchable": "^1.11",
        "spatie/laravel-settings": "^2.4",
        "spatie/laravel-tags": "^4",
        "stancl/virtualcolumn": "^1.3",
        "staudenmeir/belongs-to-through": "^2.12",
        "staudenmeir/eloquent-has-many-deep": "^1.7",
        "stechstudio/filament-impersonate": "^3.4",
        "symfony/http-client": "^6.1",
        "symfony/postmark-mailer": "^6.1",
        "unicodeveloper/laravel-paystack": "^1.0",
        "webbingbrasil/filament-advancedfilter": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "fakerphp/faker": "^1.9.1",
        "filament/upgrade": "^3.2",
        "laravel/breeze": "^1.6",
        "laravel/pint": "^1.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "@php artisan migrate --force"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
