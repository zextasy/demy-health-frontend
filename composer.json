{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "althinect/filament-spatie-roles-permissions": "dev-main",
        "archtechx/enums": "^0.3.0",
        "asantibanez/livewire-select": "^2.1",
        "doctrine/dbal": "^3.3",
        "filament/filament": "^2.10",
        "filament/spatie-laravel-media-library-plugin": "^2.0",
        "flowframe/laravel-trend": "^0.1.1",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "jantinnerezo/livewire-alert": "^2.2",
        "laravel/framework": "^8.0",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.4",
        "livewire/livewire": "^2.10",
        "pxlrbt/filament-excel": "^0.1.2",
        "saade/filament-fullcalendar": "^0.2.1",
        "spatie/laravel-activitylog": "^3.17",
        "spatie/laravel-medialibrary": "^9.11",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-ray": "^1.29",
        "spatie/laravel-searchable": "^1.11",
        "spatie/laravel-tags": "^3.1",
        "stechstudio/filament-impersonate": "^2.4",
        "webbingbrasil/filament-advancedfilter": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "beyondcode/laravel-query-detector": "^1.6",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.6",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
